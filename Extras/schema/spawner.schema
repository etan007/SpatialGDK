package unreal;

type SpawnPlayerRequest {
    string url = 1;
    bytes unique_id = 2;
    string online_platform_name = 3;
    bool simulated = 4;
    EntityId client_system_entity_id = 5;
}

type SpawnPlayerResponse { }

component PlayerSpawner {
    id = 9998;
    command SpawnPlayerResponse spawn_player(SpawnPlayerRequest);
}